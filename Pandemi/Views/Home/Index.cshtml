@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Tell Your Story LastName Family</h1>

</div>

@if (Model.Count != 0)
{

    <h1>Add your activities to the family story:</h1>
   
    <div class="btn-group" style="width:100%">
        
            <button style="width:33.3%"class="btn btn-primary btn-lg" type="button">Journal</button>
            <button style="width:33.3%"class="btn btn-success btn-lg" type="button">Foods</button>
            <button style="width:33.3%"class="btn btn-danger btn-lg" type="button">Books</button>
       
    </div>
   
    <h1></h1>
    <div class="btn-group" style="width:66.6%">
        <button style="width:33.3%"class="btn btn-warning btn-lg" type="button">Skills</button>
        <button style="width:33.3%"class="btn btn-secondary btn-lg" type="button">Future</button>
    </div>

    <div class="text-center">
        <h1 class="display-4">OR</h1>

    </div>
    <h1>Go to Private Page:</h1>
}

@foreach (var familymember in Model)
{
    <button class="btn btn-primary btn-lg" type="button">@familymember.FirstName</button>
    <h1></h1>


}
